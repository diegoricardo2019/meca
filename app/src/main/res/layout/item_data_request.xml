<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/dataRequestLayout"
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
            android:id="@+id/carDetailTextView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            tools:ignore="MissingConstraints"
            android:text="AUTO DETALLE"
            android:textStyle="bold"
            android:layout_marginStart="10px"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:textSize="50px"
            android:paddingTop="40px"
            />
    <TextView
            android:id="@+id/customerDetailTextView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            tools:ignore="MissingConstraints"
            android:text="CLIENTE DETALLE"
            android:layout_marginTop="10px"
            android:layout_marginStart="10px"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/carDetailTextView"
            />

    <TextView
            android:id="@+id/writeDetailTextView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            tools:ignore="MissingConstraints"
            android:text="DETALLE"
            android:layout_marginTop="10px"
            android:layout_marginStart="10px"
            app:layout_constraintTop_toBottomOf="@id/customerDetailTextView"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"/>

    <View
            android:id="@+id/lineSupView"
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:background="@android:color/black"
            tools:ignore="MissingConstraints"
            android:layout_marginTop="10px"
            app:layout_constraintTop_toBottomOf="@id/writeDetailTextView"/>

    <TextView
            android:id="@+id/textoFechaTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:ignore="MissingConstraints"
            android:text="FECHA :"
            android:layout_marginTop="10px"
            android:layout_marginStart="10px"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/lineSupView"/>
    <TextView
            android:id="@+id/fechaTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:ignore="MissingConstraints"
            android:text="00/00/00"
            android:layout_marginTop="10px"
            android:layout_marginEnd="10px"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/lineSupView"/>
    <TextView
            android:id="@+id/textoHoraTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:ignore="MissingConstraints"
            android:text="HORA :"
            android:layout_marginTop="10px"
            android:layout_marginStart="10px"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/textoFechaTextView"/>
    <TextView
            android:id="@+id/HoraTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:ignore="MissingConstraints"
            android:text="00:00"
            android:layout_marginTop="10px"
            android:layout_marginEnd="10px"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/fechaTextView"/>
    <View
            android:id="@+id/lineMedView"
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:background="@android:color/black"
            tools:ignore="MissingConstraints"
            android:layout_marginTop="10px"
            app:layout_constraintTop_toBottomOf="@id/HoraTextView"/>

    <TextView
            android:id="@+id/textoDomicilioTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:ignore="MissingConstraints"
            android:text="A DOMICILIO :"
            android:layout_marginTop="10px"
            android:layout_marginStart="10px"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/lineMedView"/>
    <TextView
            android:id="@+id/DomicilioTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:ignore="MissingConstraints"
            android:text="COTICACION"
            android:layout_marginTop="10px"
            android:layout_marginEnd="10px"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/lineMedView"/>

    <TextView
            android:id="@+id/textEstadoTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:ignore="MissingConstraints"
            android:text="ESTADO :"
            android:layout_marginTop="10px"
            android:layout_marginStart="10px"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/textoDomicilioTextView"/>
    <TextView
            android:id="@+id/StatusTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:ignore="MissingConstraints"
            android:text="STATUS"
            android:layout_marginTop="10px"
            android:layout_marginEnd="10px"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/DomicilioTextView"/>


    <View
            android:id="@+id/lineEndView"
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:background="@android:color/black"
            tools:ignore="MissingConstraints"
            android:layout_marginTop="10px"
            app:layout_constraintTop_toBottomOf="@id/StatusTextView"/>

</androidx.constraintlayout.widget.ConstraintLayout>